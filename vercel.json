{
  "rewrites": [
    { "source": "/api/kite/(.*)",  "destination": "/api/hub.js" },
    { "source": "/api/admin/(.*)", "destination": "/api/hub.js" },
    { "source": "/api/guardian",   "destination": "/api/hub.js" }
  ],
  "crons": [
    
    {
      "path": "/api/mtm-worker",
      "schedule": "*/1 * * * *"
    },
    {
      "path": "/api/enforce-trades",
      "schedule": "*/1 * * * *"
    },
    {
      "path": "/api/guardian",
      "schedule": "*/1 * * * *"
    }
  ]
}
